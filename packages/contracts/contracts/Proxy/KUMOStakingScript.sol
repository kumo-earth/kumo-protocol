pragma solidity 0.8.11;

import "../Dependencies/CheckContract.sol";
import "../Interfaces/IKUMOStaking.sol";


contract KUMOStakingScript is CheckContract {
    IKUMOStaking immutable KUMOStaking;

    constructor (address _kumoStakingAddress) {
        checkContract(_kumoStakingAddress);
        KUMOStaking = IKUMOStaking(_kumoStakingAddress);
    }

    function stake(uint256 _KUMOamount) external {
        KUMOStaking.stake(_KUMOamount);
    }
}
